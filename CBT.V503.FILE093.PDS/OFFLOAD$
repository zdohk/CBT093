//         DD DISP=SHR,DSN=SYS1.MODGEN
//         DD DISP=SHR,DSN=CBT.CBT492.FILE093.PDS
//SYSIN    DD DISP=SHR,DSN=SBGOLOB.B.ASM(OFFLOADW)
//SYSUT1   DD UNIT=SYSDA,SPACE=(CYL,2)
//SYSPRINT DD SYSOUT=*
//SYSLIN   DD DISP=SHR,DSN=SBGOLOB.OBJECT(OFFLOAD)
//*
//LKED   EXEC  PGM=HEWL,
//       PARM='NCAL,MAP,LIST,LET'
//SYSUT1   DD  UNIT=SYSDA,SPACE=(2048,(200,20))
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DISP=SHR,DSN=SYS1.W$$.LINKLIB
//SYSLMOD  DD  DISP=SHR,DSN=SYS1.W$$.LINKLIB
//OBJECT   DD  DISP=SHR,DSN=SBGOLOB.OBJECT(OFFLOAD)
//SYSLIN   DD  *
 INCLUDE OBJECT
